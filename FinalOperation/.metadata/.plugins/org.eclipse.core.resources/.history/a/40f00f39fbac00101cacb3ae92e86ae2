package service.impl;

import dao.ProcurementDao;
import dao.impl.ProcurementDaoImpl;
import service.ProcurementService;

public class ProcurementServiceImpl implements ProcurementService {
    private final ProcurementDao dao = new ProcurementDaoImpl();
    @Override public boolean procure(int adminEmpId, int productId, int quantity) { return dao.procure(adminEmpId, productId, quantity); }
    @Override public boolean insert(int adminEmpId, int productId, int quantity, int costUnit, int costTotal) { return dao.insert(adminEmpId, productId, quantity, costUnit, costTotal); }
}
